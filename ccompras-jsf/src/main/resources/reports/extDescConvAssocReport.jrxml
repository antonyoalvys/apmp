	<queryString>
		<![CDATA[select a.pk_associate, a.name, a.enrollment, 
case when a.main_phone is not null then a.main_phone else '' end as main_phone,
(select sum(p1.ticket_value) as total_value from purchase_ticket p1 where p1.fk_period=1)
from associate a
where 
a.pk_associate in (select distinct(p.fk_associate) from purchase_ticket p where p.fk_period=1)
order by a.name]]>
	</queryString>
