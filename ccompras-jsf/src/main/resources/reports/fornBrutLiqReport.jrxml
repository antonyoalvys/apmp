	<parameter name="PERIOD_ID" class="java.lang.Long"/>
	<parameter name="PERIOD_DESC" class="java.lang.String"/>
	<queryString>
		<![CDATA[select (c.cnpj||' - '||c.name) as company, sum(p.ticket_value) as total_value,
(select a.name from agreement a where p.fk_agreement=a.pk_agreement) as agreement,
(select r.percentage from rebate r where r.fk_company=p.fk_company and r.fk_agreement=p.fk_agreement) as percentage,
(select count(distinct(p1.fk_company, p1.fk_agreement)) from purchase_ticket p1
where p1.FK_PERIOD=$P{PERIOD_ID}) as total_items
from purchase_ticket p, company c
where 
p.fk_company=c.pk_company AND
P.FK_PERIOD= $P{PERIOD_ID} 
group by c.cnpj, c.name, agreement, percentage
order by c.name]]>
	</queryString>
