	<parameter name="PERIOD_ID" class="java.lang.Long"/>
	<parameter name="PERIOD_DESC" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.enrollment, a.name, sum(p.ticket_value) as ticket_value,
(select sum(p1.ticket_value) as total_ticket_value from purchase_ticket p1 where p1.fk_period= $P{PERIOD_ID}  ) ,
(select count(distinct p2.fk_associate) total_tickets from purchase_ticket p2 where p2.fk_period=$P{PERIOD_ID})
from purchase_ticket p, associate a
where 
p.fk_period= $P{PERIOD_ID} 
and p.fk_associate=a.pk_associate
group by a.enrollment, a.name]]>
	</queryString>
